<tr>
  <td> <%= user.login %> </td>
  <td> <%= t("users.attr.language.#{user.language}") %> </td>
  <td> <%= user.created_at %> </td>
  <td> <%= link_to t('shared.link_show'), staff_user_path(user) %> </td>
  <td> <%= link_to t('shared.link_edit'), edit_staff_user_path(user) %> </td>
  <td> <%= button_to t('shared.link_blacklist'), staff_user_path(user), method: :delete %> </td>
</tr>
