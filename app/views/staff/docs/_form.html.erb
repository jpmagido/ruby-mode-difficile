<%= link_to t('shared.back'), :back %>
<br>
<hr>

<div>
  <%= form_with model: array_model do |f| %>
    <%= f.label :title, t('docs.attr.title') %>
    <br>
    <%= f.text_field :title %>
    <br>
    <br>

    <%= f.label :tags, t('docs.attr.tags') %>
    <br>
    <%= f.text_field :tags %>
    <br>
    <br>
    <%= f.label :content_fr, t('docs.attr.content-fr') %>
    <br>
    <%= f.rich_text_area :content_fr %>
    <br>
    <br>
    <%= f.label :content_en, t('docs.attr.content-en') %>
    <br>
    <%= f.rich_text_area :content_en %>
    <br>
    <br>

    <%= f.submit t('shared.submit_form') %>
  <% end %>
</div>
