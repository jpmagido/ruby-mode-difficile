<h3>Mentor::Mentorships#show</h3>

<br>
<div> <%= link_to t('shared.back'), :back %> </div>
<div> <%= link_to t('mentor.mentorship_sessions.links.create-mentorship-session'), new_mentor_mentorship_session_path(mentorship_id: mentorship.id) %> </div>
<hr>
<br>

<table class='table'>
  <thead>
  <tr>
    <th scope='col'> <%= t('mentor.mentorships.attr.coach-name') %> </th>
    <th scope='col'> <%= t('mentor.mentorships.attr.student-name') %> </th>
    <th scope='col'> <%= t('mentor.mentorships.attr.coach-approval') %> </th>
    <th scope='col'> <%= t('mentor.mentorships.attr.student-approval') %> </th>
    <th scope='col'> <%= t('shared.attr.created-at') %> </th>
    <th scope='col'> <%= t('shared.attr.updated-at') %> </th>
    <th scope='col'> <%= t('shared.link_edit') %> </th>
  </tr>
  </thead>
  <tbody> <%= render mentorship %> </tbody>
</table>
