<div class="container">
  <h2>Mentor::MentorshipSessions#new</h2>
  <div>
    <%= form_with model: [:mentor, @new_mentorship_session] do |f| %>
      <%= f.label :start_date, t('mentor.mentorship_sessions.attr.start_date') %>
      <%= f.date_select :start_date %>
      <br>
      <%= f.label :end_date, t('mentor.mentorship_sessions.attr.end_date') %>
      <%= f.datetime_select :end_date %>
      <br>
      <%= f.hidden_field :mentorship_id, value: params[:mentorship_id] %>
      <br>
      <%= f.submit t('shared.submit_form') %>
    <% end %>
  </div>

</div>
